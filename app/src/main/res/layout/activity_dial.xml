<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dial">

    <TextView
        android:textIsSelectable="true"
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ubermovemedium"
        android:gravity="center"
        android:text="Contacts"
        android:textColor="#000"
        android:textSize="45sp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_physician"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/txt_physician"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:gravity="center"
        android:text="Physician"
        android:textColor="#000"
        android:onClick="physician"
        android:textSize="30dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_instructor"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/txt_instructor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:gravity="center"
        android:text="Instructor"
        android:onClick="instructor"
        android:textColor="#000"
        android:textSize="30dp"
        app:layout_constraintTop_toTopOf="@+id/guideline30"
     />

    <TextView
        android:id="@+id/txt_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:gravity="center"
        android:text="Home"
        android:onClick="home"
        android:layout_marginTop="20dp"
        android:textColor="#000"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_instructor"
        tools:layout_editor_absoluteX="16dp" />


    <TextView

        android:id="@+id/developer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/uber_move_bolditalic"

        android:textColor="#000"
        android:textSize="20dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>